{
  "permissions": {
    "allow": [
      "Bash(git log:*)",
      "Bash(git grep:*)",
      "Bash(for f in doc/*.md)",
      "Bash(do echo \"=== $f ===\")",
      "Bash(head:*)",
      "Bash(xargs ls:*)",
      "Bash(clj-kondo --lint:*)",
      "Bash(cljfmt fix:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(./run_tests.bb:*)",
      "Bash(git push:*)",
      "Bash(git tag:*)"
    ],
    "deny": [],
    "ask": []
  },
  "enableAllProjectMcpServers": true,
  "enabledMcpjsonServers": [
    "memory"
  ]
}
